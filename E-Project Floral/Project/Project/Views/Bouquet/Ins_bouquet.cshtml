@{
    Layout = "../Shared/_Layout.cshtml";
}
<div class="main-panel">
    <div class="content-wrapper pb-0">
        <div class="page-header flex-wrap">
            <h3 class="mb-0">
               Insert Bouquets and Products Here
            </h3>
            
        </div>
        <div class="col-md-12 grid-margin stretch-card">
            <div class="card">
                <div class="card-body">
                    <h4 class="card-title">Bouquets insertion form</h4>
                    <p class="card-description">Add Bouquets here</p>
                    <form class="forms-sample" method="POST" action="@Url.Action("ins_bouquet","Bouquet")" enctype="multipart/form-data">
                        <div class="form-group">
                            <label for="exampleInputUsername1">Name</label>
                            <input type="text" class="form-control" id="exampleInputUsername1" asp-for="name" placeholder="name" name="name" />
                        </div>
                        <div class="form-group">
                            <label for="exampleInputEmail1">Price</label>
                            <input type="text" class="form-control" id="exampleInputEmail1" asp-for="price" placeholder="Price" name="price"/>
                        </div>
                        <div class="form-group">
                            <label for="exampleInputPassword1">Image</label>
                            <input type="file" name="image" value="image" asp-for="image" class="form-control" id="exampleInputPassword1" placeholder="Insert image" />
                        </div>
                        
                        
                        <button type="submit" value="submit" class="btn btn-primary mr-2"> Submit </button>
                        <button class="btn btn-light">Cancel</button>
                    </form>
                </div>
            </div>
        </div>
    </div>
    <!-- main-panel ends -->
</div>
<!-- page-body-wrapper ends -->
    </div>
